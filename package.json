{
    "name": "node-message-broker",
    "version": "0.1.0",
    "description": "",
    "author": {
        "name": "PrivateAIM",
        "url": "https://github.com/PrivateAIM"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PrivateAIM/node-message-broker.git"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && rimraf bin && npm run build:types && npm run build:js",
        "build:watch": "npm run build -- --watch",
        "test": "jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "husky install",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/index"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@nestjs/common": "^10.3.3",
        "@nestjs/config": "^3.2.0",
        "@nestjs/core": "^10.3.3",
        "@nestjs/mongoose": "^10.0.4",
        "@nestjs/platform-express": "^10.3.3",
        "@nestjs/terminus": "^10.2.2",
        "@types/express": "^4.17.21",
        "@types/uuid": "^9.0.8",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "express": "^4.18.2",
        "jose": "^5.2.1",
        "mongoose": "^8.1.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.3.2",
        "@nestjs/schematics": "^10.1.1",
        "@nestjs/testing": "^10.3.3",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-swc": "^0.3.0",
        "@swc/jest": "^0.2.36",
        "@tada5hi/commitlint-config": "^1.1.7",
        "@tada5hi/eslint-config-typescript": "^1.2.9",
        "@tada5hi/semantic-release": "^0.3.1",
        "@tada5hi/tsconfig": "^0.5.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.21",
        "@types/supertest": "^6.0.2",
        "axios": "^1.6.7",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "rollup": "^4.11.0",
        "semantic-release": "^23.0.2",
        "supertest": "^6.3.4",
        "testcontainers": "^10.7.1",
        "typescript": "^5.3.3"
    },
    "lint-staged": {
        ".js": "npm run lint:fix",
        "*.ts": "npm run lint:fix"
    }
}
