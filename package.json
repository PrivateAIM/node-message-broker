{
    "name": "node-message-broker",
    "version": "0.1.0",
    "description": "",
    "author": {
        "name": "PrivateAIM",
        "url": "https://github.com/PrivateAIM"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PrivateAIM/node-message-broker.git"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && rimraf bin && npm run build:types && npm run build:js",
        "build:watch": "npm run build -- --watch",
        "test": "jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "husky install",
        "start:dev": "nodemon --exec ts-node -- ./src/index.ts",
        "start:prod": "NODE_ENV=production node dist/index.cjs"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@authup/core-http-kit": "^1.0.0-beta.19",
        "@authup/core-realtime-kit": "^1.0.0-beta.19",
        "@effect/platform-node": "^0.51.11",
        "@effect/schema": "^0.68.0",
        "@privateaim/core-http-kit": "^0.8.0",
        "@privateaim/core-kit": "^0.8.0",
        "@privateaim/messenger-kit": "^0.7.1",
        "@types/express": "^4.17.21",
        "@types/uuid": "^9.0.8",
        "axios": "^1.7.2",
        "effect": "^3.3.0",
        "express": "^4.18.3",
        "jose": "^5.4.0",
        "mongoose": "^8.6.0",
        "socket.io-client": "^4.7.5",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-swc": "^0.3.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@swc/jest": "^0.2.36",
        "@tada5hi/commitlint-config": "^1.2.1",
        "@tada5hi/tsconfig": "^0.5.1",
        "@types/node": "^20.14.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.2.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.6",
        "nodemon": "^3.1.3",
        "rimraf": "^6.0.1",
        "rollup": "^4.18.0",
        "semantic-release": "^24.0.0",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.13.0"
    },
    "lint-staged": {
        ".js": "npm run lint:fix",
        "*.ts": "npm run lint:fix"
    }
}
