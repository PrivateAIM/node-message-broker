{
    "name": "node-message-broker",
    "version": "0.1.0",
    "description": "",
    "author": {
        "name": "PrivateAIM",
        "url": "https://github.com/PrivateAIM"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PrivateAIM/node-message-broker.git"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && rimraf bin && npm run build:types && npm run build:js",
        "build:watch": "npm run build -- --watch",
        "test": "jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint",
        "lint:fix": "npm run lint -- --fix",
        "prepare": "husky install",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/index"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@authup/core": "^1.0.0-beta.9",
        "@nestjs/common": "^10.3.9",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.3.9",
        "@nestjs/event-emitter": "^2.0.4",
        "@nestjs/mongoose": "^10.0.6",
        "@nestjs/platform-express": "^10.3.9",
        "@nestjs/terminus": "^10.2.3",
        "@privateaim/core": "^0.4.0",
        "@types/express": "^4.17.21",
        "@types/uuid": "^9.0.8",
        "axios": "^1.7.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "express": "^4.18.3",
        "jose": "^5.4.0",
        "mongoose": "^8.4.1",
        "socket.io-client": "^4.7.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@nestjs/cli": "^10.3.2",
        "@nestjs/schematics": "^10.1.1",
        "@nestjs/testing": "^10.3.9",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-swc": "^0.3.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@swc/jest": "^0.2.36",
        "@tada5hi/commitlint-config": "^1.2.1",
        "@tada5hi/tsconfig": "^0.5.1",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.12",
        "@types/jest-when": "^3.5.5",
        "@types/node": "^20.14.2",
        "@types/supertest": "^6.0.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.2.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-when": "^3.6.0",
        "lint-staged": "^15.2.6",
        "nock": "^13.5.4",
        "rollup": "^4.18.0",
        "semantic-release": "^24.0.0",
        "supertest": "^7.0.0",
        "testcontainers": "^10.9.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.13.0"
    },
    "lint-staged": {
        ".js": "npm run lint:fix",
        "*.ts": "npm run lint:fix"
    }
}
